extends layout
block content
    -var row =1;
    h1=title
    
    table(class="w3-table-all w3-hoverable")
        tr(class="w3-red")
            th
            th Titel
            th Artiest
            th Stemmen
            each liedje in liedje_list
                tr
                    td #{row++} 
                    td #{liedje.titel}
                    td #{liedje.artiestNaam}
                    td #{liedje.aantStemmen}